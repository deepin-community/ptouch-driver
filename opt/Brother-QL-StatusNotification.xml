<option type='enum' id='opt/Brother-QL-StatusNotification'>
  <comments>
    <en>This is a printer specific option.</en>
  </comments>
  <arg_longname>
    <en>Automatic Status Notification</en>
  </arg_longname>
  <arg_shortname>
    <en>StatusNotification</en><!-- backends only know <en> shortnames! -->
  </arg_shortname>
  <arg_execution>
    <arg_group>PrinterSpecifics</arg_group>
    <arg_order>100</arg_order>
    <arg_spot>B</arg_spot>
    <arg_substitution />
    <arg_proto> StatusNotification=%s </arg_proto>
  </arg_execution>
  <enum_vals>
   <enum_val id="ev/1">
    <ev_longname>
     <en>Do not notify</en>
    </ev_longname>
    <ev_shortname>
     <en>DoNotNotify</en>
    </ev_shortname>
    <ev_driverval>1</ev_driverval>
   </enum_val>
  </enum_vals>
</option>
