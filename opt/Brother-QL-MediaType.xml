<option type="enum" id="opt/Brother-QL-MediaType">
  <arg_longname>
   <en>Media Type</en>
  </arg_longname>
  <arg_shortname>
   <en>MediaType</en><!-- backends only know <en> shortnames! -->
  </arg_shortname>
  <arg_execution>
    <arg_group>PrinterSpecifics</arg_group>
    <arg_order>100</arg_order>
    <arg_spot>B</arg_spot>
    <arg_substitution />
   <arg_proto> MediaType=%s </arg_proto>
  </arg_execution>
  <constraints>
    <constraint sense='true'>
      <driver>ptouch-ql</driver>
      <arg_defval>ev/Tape</arg_defval>
    </constraint>
  </constraints>
  <enum_vals>
   <enum_val id="ev/Tape">
    <ev_longname>
     <en>Continuous-Length Tape</en>
    </ev_longname>
    <ev_shortname>
     <en>Tape</en>
    </ev_shortname>
    <ev_driverval>Tape</ev_driverval>
   </enum_val>
   <enum_val id="ev/Labels">
    <ev_longname>
     <en>Die-Cut Labels</en>
    </ev_longname>
    <ev_shortname>
     <en>Labels</en>
    </ev_shortname>
    <ev_driverval>Labels</ev_driverval>
   </enum_val>
  </enum_vals>
</option>
